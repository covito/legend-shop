<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--  LegendShop 版权所有 2009-2011,并保留所有权利。 -->
<hibernate-mapping>
    <class name="com.legendshop.model.entity.DynamicTemp" table="ls_dyn_temp">
        <id name="id" type="java.lang.Long">
            <column name="id" />
            <generator class="native" />
        </id>
       <property name="name" type="java.lang.String">
            <column name="name">
                <comment>名称</comment>
            </column>
        </property>
        <property name="content" type="java.lang.String">
            <column name="content">
                <comment>内容</comment>
            </column>
        </property>
        <property name="type" type="java.lang.Integer">
            <column name="type">
                <comment>类型，1:attributeute,2:parameter</comment>
            </column>
        </property>
        <property name="userName" type="java.lang.String">
            <column name="user_name">
                <comment>用户名</comment>
            </column>
        </property>
         <property name="sortId" type="java.lang.Long">
            <column name="sort_id">
                <comment>商品分类ID</comment>
            </column>
        </property>
         <property name="status" type="java.lang.Integer">
            <column name="status">
                <comment>默认是1，表示正常状态,0为下线状态</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
