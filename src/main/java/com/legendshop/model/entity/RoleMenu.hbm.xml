<hibernate-mapping>
	<class name="com.legendshop.model.entity.RoleMenu" table="ls_role_menu">
		<id name="rmId" column="rm_id" type="java.lang.Long">
			<generator class="native" />
		</id>
    	
    		<property name="roleId" column="role_id" type="java.lang.String" />
    	
    		<property name="menuId" column="menu_id" type="java.lang.Long" />
	</class>
</hibernate-mapping>
