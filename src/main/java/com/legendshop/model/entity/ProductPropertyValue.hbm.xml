<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
 * 
 * LegendShop 多用户商城系统
 * 
 *  版权所有,并保留所有权利。
 * 
  -->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.ProductPropertyValue" table="ls_prod_prop_value">
		<id name="valueId" column="value_id" type="java.lang.Long">
			<generator class="native" />
		</id>
    	<property name="propId" column="prop_id" type="java.lang.Long" />
    	<property name="name" column="name" type="java.lang.String" />
    	<property name="status" column="status" type="java.lang.Short" />
    	<property name="pic" column="pic" type="java.lang.String" />
    	<property name="sequence" column="sequence" type="java.lang.Long" />
    	<property name="modifyDate" column="modify_date" type="java.util.Date" />
    	<property name="recDate" column="rec_date" type="java.util.Date" />
	</class>
</hibernate-mapping>
