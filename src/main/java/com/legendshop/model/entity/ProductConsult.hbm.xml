<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
	@author He-WenQiang. Create in 2013-04-05 18:28:25
-->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.ProductConsult" table="ls_prod_cons">
	<id name="consId" column="cons_id" type="java.lang.Long">
     <generator class="native" />
	</id>
	<property name="prodId" column="prod_id"  type="java.lang.Long" />
	<property name="pointType" column="point_type"  type="java.lang.Integer" />
	<property name="content" column="content" type="java.lang.String" />
	<property name="answer" column="answer" type="java.lang.String" />
	<property name="recDate" column="rec_date" type="java.util.Date" />
	<property name="postip" column="postip" type="java.lang.String" />
	<property name="answertime" column="answertime" type="java.util.Date" />
	<property name="askUserName" column="ask_user_name" type="java.lang.String" />
	<property name="ansUserName" column="ans_user_name" type="java.lang.String" />
	</class>
</hibernate-mapping>
