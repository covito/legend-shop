<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--  LegendShop 版权所有 2009-2011,并保留所有权利。 -->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.ImgFile" table="ls_img_file">
	<id name="fileId" column="file_id" type="java.lang.Long">
     <generator class="native" />
	</id>
	<property name="userName" column="user_name" type="java.lang.String" />
	<property name="productId" column="product_id" type="java.lang.Long" />
	<property name="productType" column="product_type" type="java.lang.Short" />
	<property name="filePath" column="file_path" type="java.lang.String" />
	<property name="fileType" column="file_type" type="java.lang.String" />
	<property name="fileSize" column="file_size" type="java.lang.Integer" />
	<property name="upoadTime" column="upoad_time" type="java.util.Date" />
	<property name="status" column="status" type="java.lang.Integer" />

	</class>
</hibernate-mapping>
