<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
 * 
 * LegendShop 多用户商城系统
 * 
 *  版权所有,并保留所有权利。
 * 
  -->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.Menu" table="ls_menu">
		<id name="menuId" column="menu_id" type="java.lang.Long">
			<generator class="native" />
		</id>
    	
    		<property name="name" column="name" type="java.lang.String" />
    	
    		<property name="label" column="label" type="java.lang.String" />
    	
    		<property name="title" column="title" type="java.lang.String" />
    	
    		<property name="action" column="action" type="java.lang.String" />
    	
    		<property name="parentId" column="parent_id" type="java.lang.Long" />
    	
    		<property name="level" column="level" type="java.lang.Integer" />
    	
    		<property name="providedPlugin" column="provided_plugin" type="java.lang.String" />
    	
    		<property name="seq" column="seq" type="java.lang.Integer" />
	</class>
</hibernate-mapping>
