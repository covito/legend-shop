<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
 * 
 * LegendShop 多用户商城系统
 * 
 *  版权所有,并保留所有权利。
 * 
  -->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.Pub" table="ls_pub">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>
    	
    		<property name="title" column="title" type="java.lang.String" />
    	
    		<property name="msg" column="msg" type="java.lang.String" />
    	
    		<property name="recDate" column="rec_date" type="java.util.Date" />
    	
    		<property name="startDate" column="start_date" type="java.util.Date" />
    	
    		<property name="endDate" column="end_date" type="java.util.Date" />
    	
    		<property name="userId" column="user_id" type="java.lang.String" />
    	
    		<property name="userName" column="user_name" type="java.lang.String" />
    	
    		<property name="status" column="status" type="java.lang.Integer"  />
	</class>
</hibernate-mapping>
