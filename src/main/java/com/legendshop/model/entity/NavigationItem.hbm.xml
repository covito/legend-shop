<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
 * 
 * LegendShop 多用户商城系统
 * 
 *  版权所有,并保留所有权利。
 * 
  -->
<hibernate-mapping>
	<class name="com.legendshop.model.entity.NavigationItem" table="ls_navi_item">
		<id name="itemId" column="item_id" type="java.lang.Long">
			<generator class="native" />
		</id>
    	
    		<property name="naviId" column="navi_id" type="java.lang.Long" />
    	
    		<property name="name" column="name" type="java.lang.String" />
    	
    		<property name="link" column="link" type="java.lang.String" />
    	
    		<property name="seq" column="seq" type="java.lang.Long" />
    	
    		<property name="status" column="status" type="java.lang.Integer" />
	</class>
</hibernate-mapping>
